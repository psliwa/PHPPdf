<?xml version="1.0" encoding="UTF-8"?>
<stylesheet>
	<div class="cover" background.image="dir:/zend-studio/cover.jpg" background.image-width="100%" height="532px" color="white" vertical-align="top">
		<p margin="66px 66px" font-size="24px">
			<h1 font-size="41px" font-style="normal" padding="0" margin="0"></h1>
		</p>
	</div>
	
	<dynamic-page margin="52px 93px">

		<h1 font-size="40px" font-style="normal" color="#00709e"></h1>
		<h2 color="#00709e"></h2>
		<p font-size="14.5px" line-height="24px"></p>
		<li font-size="14.5px" line-height="24px"></li>
		<div font-size="14.5px" line-height="24px"></div>
		<ul list-position="inside" type="circle" margin-left="27px">
			<li margin-top="3px" margin-bottom="3px" />
			<ul margin-left="0" />
		</ul>
		
		<div class="note" margin-top="6.5px">
			<div class="header" background.color="#838383" color="white" font-style="bold" padding="3px">
			</div>
			<div class="content" background.color="#e5e5e5" padding="3px">
			</div>
		</div>
		
		<div class="margin" height="27px"></div>
		
		<table margin-top="6.5px">
			<tr class="header" font-style="bold" />
			<td padding="6.5px" border.size="1px" />
			<ul margin-left="0" />
		</table>
		
		<div class="image" width="75%" margin="0 auto" text-align="center">
			<img width="100%" />
			<p font-size="12px" />
		</div>
		
		<column-layout>
			<p line-height="21px" padding="0" />
		</column-layout>
	</dynamic-page>
</stylesheet>